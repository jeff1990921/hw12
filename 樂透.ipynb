      "source": [
        "import random\n",
        "def lottery():\n",
        "  result = []\n",
        "  while len(result) < 6:\n",
        "    n = random.randint(1,42)\n",
        "    if n not in result:\n",
        "      result.append(n)\n",
        "  return sorted(result)"
      ],
      "execution_count": 0,
      "outputs": []
